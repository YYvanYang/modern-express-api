{
  "name": "modern-express-api",
  "version": "1.0.0",
  "description": "Modern Express API with TypeScript, Drizzle ORM and Docker",
  "main": "build/server.js",
  "scripts": {
    "build": "rimraf build && tsc",
    "start": "node build/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:generate": "drizzle-kit generate:pg",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "db:wait": "ts-node scripts/wait-for-db.ts",
    "db:migrate": "npm run db:wait && drizzle-kit push:pg",
    "db:seed": "ts-node scripts/seed.ts",
    "db:seed:dev": "ts-node scripts/seed/dev.ts",
    "db:reset": "ts-node scripts/seed/resetDb.ts",
    "db:refresh": "npm run db:reset && npm run db:seed",
    "db:refresh:dev": "npm run db:reset && npm run db:seed:dev",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:ps": "docker compose ps",
    "docker:dev": "docker compose watch",
    "docker:prune": "docker compose down -v --remove-orphans",
    "prod:build": "docker compose -f compose.prod.yml build",
    "prod:up": "docker compose -f compose.prod.yml up -d",
    "prod:down": "docker compose -f compose.prod.yml down",
    "prod:logs": "docker compose -f compose.prod.yml logs -f",
    "prod:migrate": "cross-env NODE_ENV=production npm run db:migrate"
  },
  "dependencies": {
    "@anatine/zod-openapi": "^2.2.6",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.3",
    "drizzle-zod": "^0.5.1",
    "express": "^5.0.1",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "openapi3-ts": "^4.4.0",
    "pg": "^8.13.1",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.28.1",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "express",
    "typescript",
    "drizzle",
    "docker",
    "postgresql",
    "api"
  ],
  "author": "Your Name",
  "license": "MIT"
}